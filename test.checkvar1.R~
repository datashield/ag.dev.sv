servers <- vector("list", 3)

dt1 <- cbind(c(0,0,0),c(1,1,1), c(2,2,2))
#dt1[,1] <- NA
dt1[,3] <- NA
colnames(dt1) <- c("c1", "c2", "c3")
dt2 <- cbind(c(1,1,1), c(2,2,2))
colnames(dt2) <- c("c1", "c3")
dt3 <- cbind(c(0,0,0),c(1,1,1), c(2,2,2))
colnames(dt3) <- c("c1", "c2", "c3")

servers[[1]] <- dt1
servers[[2]] <- dt2
servers[[3]] <- dt3

names(servers) <- c("std1", "std2", "std3")

f <- as.formula(D$c1~D$c2+D$c3)

opals <- servers

source("ag.ds.checkvar1.R")

